{ 
    "name": "book finder",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": 
        {
            "default_popup": "popup.html",
            "default_title": "book finder"
        },
    "permissions":
    [
        "tabs",
        "input",
        "activeTab",
        "<all_urls>"
    ]
}